<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="mainContent" prefHeight="673.0" prefWidth="990.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asla_client.controller.UserController">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="96.0" prefWidth="220.0" spacing="15.0">
         <children>
            <TextField fx:id="textInput" promptText="Search city" />
            <HBox prefHeight="25.0" prefWidth="229.0" spacing="5.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="requestBtn" mnemonicParsing="false" onAction="#request" prefWidth="100.0" text="Search" />
               </children>
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
      <LineChart fx:id="lineGraph" layoutX="15.0" layoutY="157.0" prefHeight="492.0" prefWidth="951.0" title="Graph">
        <xAxis>
          <NumberAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</Pane>
