<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="720.0" prefWidth="1024.0" stylesheets="@basicStyleSheet.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="asla_client.controller.PrimaryController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
   <padding>
      <Insets bottom="5.0" />
   </padding>
   <left>
      <VBox alignment="TOP_CENTER" prefHeight="614.0" prefWidth="217.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="128.0" prefWidth="207.0" spacing="40.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <TextField fx:id="textInputUsername" promptText="SSN (username)" />
            <TextField fx:id="textInputPassword" promptText="password" />
            <HBox prefHeight="25.0" prefWidth="211.0" spacing="5.0">
               <children>
                  <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#login" text="login" />
                  <HBox prefHeight="25.0" prefWidth="152.0">
                     <children>
                        <Button fx:id="registerBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="63.0" text="Register" />
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets left="5.0" />
               </VBox.margin>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </left>
   <right>
      <Region prefHeight="690.0" prefWidth="15.0" BorderPane.alignment="CENTER" />
   </right>
   <bottom>
      <Region prefHeight="14.0" prefWidth="1024.0" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="textArea" editable="false" layoutX="75.0" layoutY="91.0" prefHeight="409.0" prefWidth="554.0" wrapText="true" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
